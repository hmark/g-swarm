	}

	public void onBulletHit(BulletHitEvent e) {
		#gswarm-bullethit.grm
	}

	public void onHitByBullet(HitByBulletEvent e) {
		#gswarm-hitbybullet.grm
	}

	private int random(){
		return generator.nextInt(800);
	}

}